{
  "$schema": "http://json.schemastore.org/template",
  "author": "Meysam Bostak",
  "classifications": ["MVC", ".NET Core", "ASP.NET Core"],
  "name": "Empty Daric Template Framework project",
  "identity": "Daric.Framework",
  "shortName": "darictemplate",
